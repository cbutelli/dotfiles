# =========================================== #
# ===== Generic Shell agnostic settings ===== #
# =========================================== #

# ===== Default EDITOR to vi ===== #
export EDITOR=vi

source ~/dotfiles/common/vars.sh
source ~/dotfiles/common/paths.sh
source ~/dotfiles/common/aliases.sh

# Source global definitions
if [ -f ~/.custom_env.sh ]; then
    . ~/.custom_env.sh
fi
